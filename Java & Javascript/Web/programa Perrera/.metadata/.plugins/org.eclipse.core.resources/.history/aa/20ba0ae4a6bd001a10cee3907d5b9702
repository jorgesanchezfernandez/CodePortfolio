package Animal.Tipo;

import Animal.animal;
import java.util.List;

public class ave extends animal{
	

	private String especie;
	private String colorPlumas;
	private int envergadura;
	private String tipoVuelo;
	
		
	public String getColorPlumas() {
		return colorPlumas;
	}
	public void setColorPlumas(String colorPlumas) {
		this.colorPlumas = colorPlumas;
	}
	public int getEnvergadura() {
		return envergadura;
	}
	public void setEnvergadura(int envergadura) {
		this.envergadura = envergadura;
	}
	public String getEspecie() {
		return especie;
	}
	public void setEspecie(String especie) {
		this.especie = especie;
	}
	public String getTipoVuelo() {
		return tipoVuelo;
	}
	public void setTipoVuelo(String tipoVuelo) {
		this.tipoVuelo = tipoVuelo;
	}
	
	public List<String> getDatos(){
		List<String> result=super.getDatos();
		
		result.add(especie);
		result.add(colorPlumas);
		result.add(Integer.toString(envergadura));
		result.add(tipoVuelo);
		
		return result;
	}
}
